    "# Load the CSV into a Dask DataFrame\n",
    "df = dd.read_csv('timeseries.csv', dtype={'county': 'object', 'state': 'object', 'country': 'object', \n",
    "           'population': 'float64', 'cases': 'float64', 'deaths': 'float64'}\n",
    ")\n",
    "df_us = df[(df['country'] == 'United States') & (df['state'].notnull()) & (df['county'].isnull())]"
